      "src/bin/test-bootstrap.ts",
      "src/**/*.spec.ts"
    ]
  },
  "_moduleDirectories": [
    "dist"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.141",
    "@types/mocha": "^5.2.7",
    "@types/mockery": "^1.4.29",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.7.8",
    "@types/request-promise": "^4.1.44",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "@types/uniqid": "^4.1.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.2",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "request": "^2.88.0",
